CREATE TABLE MEMBER_TBL_02 (
CUSTNO NUMBER(6) PRIMARY KEY,
CUSTNAME VARCHAR2(20),
PHONE VARCHAR2(13),
ADDRESS VARCHAR2(60),
JOINDATE DATE,
GRADE CHAR(1),
CITY CHAR(2)
);

CREATE TABLE MONEY_TBL_02 (
CUSTNO NUMBER(6),
SALENOL NUMBER(8),
PCOST NUMBER(8),
AMOUNT NUMBER(4),
PRICE NUMBER(8),
PCODE VARCHAR2(4),
SDATE DATE,
PRIMARY KEY(CUSTNO, SALENOL)
);

